{
  "name": "automationtest",
  "version": "1.0.0",
  "description": "Automation Testing - Cypress",
  "type": "module",
  "scripts": {
    "clean:reports": "rmdir /S /Q cypress\\reports && mkdir cypress\\reports && mkdir cypress\\reports\\mochareports",
    "pretest": "npm run clean:reports",
    "scripts": "cypress run",
    "combine-reports": "mochawesome-merge cypress/reports/mocha/*.json > cypressreports/mochareports/report.json",
    "generate-report": "marge cypress/reports/mochareports/ report.json -f report -o cypress/reports/mochareports",
    "posttest": "npm run combine-reports && npm run generate-report",
    "cypress-e2e": "npm run scripts || npm run posttest",
    "cypress-component": "cypress run --component"
  },
  "author": "Reymous F. Guballo",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^13.11.0",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "vite": "^5.2.12"
  },
  "dependencies": {
    "@types/prop-types": "^15.7.12",
    "@vitejs/plugin-react": "^4.3.0",
    "bootstrap": "^5.3.2",
    "cypress-downloadfile": "^1.2.3",
    "cypress-file-upload": "^5.0.8",
    "cypress-mochawesome-reporter": "^3.8.2",
    "cypress-multi-reporters": "^1.6.4",
    "cypress-real-events": "^1.12.0",
    "date-fns": "^3.6.0",
    "mochawesome": "^7.1.3",
    "moment": "^2.30.1",
    "prop-type": "^0.0.1",
    "prop-types": "^15.8.1",
    "react-bootstrap": "^2.10.2"
  }
}
